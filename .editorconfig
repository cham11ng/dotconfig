# editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.bat]
indent_style = tab
end_of_line = crlf

[*.py]
indent_size = 4

[Makefile]
indent_size = 4
indent_style = tab

[LICENSE]
insert_final_newline = false
